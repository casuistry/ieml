
extends nglayout

block body
    div
     h2 My Blog
     ul
          li
           a(href='/') Home
          li
            a(href='/second') Show view #2


    h1= title
    p Welcome..... to #{title}

    
    div(ng-controller='FirstController')
      label ieml filter only 
               input(ng-model='search.ieml')
           p List of IEML
           ul.page-sidebar-menu(ng-repeat='item in List | filter:search')
              #searchObjResults
                li(ng-class='group.groupStyle')
                   p
                       span.title {{item.ieml}}
                     
    div(ng-view)

    script(src='../libs/angular/angular.js')
    script(src='../libs/angular-route/angular-route.js')
    script(src='../js/controllers/Contr1.js')
    script(src='../js/services/AlliemlService.js')
    script(src='../js/appRoutes.js')
    script(src='../js/app.js')
               


   

